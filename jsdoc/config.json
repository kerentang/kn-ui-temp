{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./"],
    "exclude": ["./node_modules", "./builds", "./.jsdoc", "./dist"],
    "includePattern": ".+\\.(js|scss|vue)$"
  },
  "plugins": ["./plugins/commentOnly"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "./.jsdoc"
  }
}
